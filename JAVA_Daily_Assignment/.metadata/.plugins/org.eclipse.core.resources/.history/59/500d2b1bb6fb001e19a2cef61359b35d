package day0415.ex5;

public class IntMinMaxValueTest {

	public static void main(String[] args) {
		
		// 자바의 정수 타입의 최대값과 최소값
		System.out.println("byte 정수 최소값: " + Byte.MAX_VALUE + ", 최대값: " + Byte.MIN_VALUE);
		System.out.println("short 정수 최소값: " + Short.MAX_VALUE + ", 최대값: " + Short.MIN_VALUE);
		System.out.println("int 정수 최소값: " + Integer.MAX_VALUE + ", 최대값: " + Integer.MIN_VALUE);
		System.out.println("long 정수 최소값: " + Long.MAX_VALUE + ", 최대값: " + Long.MIN_VALUE);
	}

}
